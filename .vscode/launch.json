      "args": [
        "--out-dir",
        "/root/workspace/master/wasm-wx-demo/pkg",
        "--target",
        "web",
        "--no-typescript",
        "/root/workspace/master/wasm-wx-demo/target/wasm32-unknown-unknown/debug/wasm_wx_demo.wasm"
      ]
    }
  ]
}
